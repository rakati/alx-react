{
	"env": {
	  "browser": true,
	  "es2020": true,
	  "node": true
	},
	"extends": [
	  "eslint:recommended",
	  "plugin:react/recommended"
	],
	"parserOptions": {
	  "ecmaVersion": 11,
	  "sourceType": "module",
	  "ecmaFeatures": {
		"jsx": true
	  }
	},
	"plugins": ["react"],
	"rules": {
	  // Define custom rules here
	  "react/prop-types": "off"
	}
}
